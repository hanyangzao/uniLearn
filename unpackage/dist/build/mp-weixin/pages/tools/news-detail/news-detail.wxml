<view class="content"><block wx:if="{{newsDetail}}"><view class="hasData"><view class="otherInfo"><view class="dot"></view><text class="source">{{newsDetail.source}}</text><text class="ptime">{{newsDetail.ptime}}</text></view><view class="newsTitle">{{''+newsDetail.title+''}}</view><block wx:if="{{newsDetail.images.length}}"><swiper class="swiper" indicator-dots="{{swiperOption.indicatorDots}}" autoplay="{{swiperOption.autoplay}}" interval="{{swiperOption.interval}}" duration="{{swiperOption.duration}}" circular="{{swiperOption.circular}}"><block wx:for="{{newsDetail.images}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item data-event-opts="{{[['tap',[['previewImage',['$0'],[[['newsDetail.images','',index,'imgSrc']]]]]]]}}" bindtap="__e"><view class="swiper-item"><image src="{{item.imgSrc}}"></image></view></swiper-item></block></swiper></block></view></block><block wx:else><view class="noData">无数据</view></block><view class="newsdetail"><u-parse vue-id="b1f12ec8-1" content="{{newsDetail.content}}" data-event-opts="{{[['^navigate',[['navigate']]]]}}" bind:navigate="__e" bind:__l="__l"></u-parse></view></view>